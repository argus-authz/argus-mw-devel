[SERVICE]
entityId = https://<%= @fqdn %>/pdp
hostname = <%= @fqdn %>
port = <%= @pdp_port %>
adminHost = <%= @pdp_admin_host %>
adminPort = <%= @pdp_admin_port %>
adminPassword = <%= @pdp_admin_password %>

[POLICY]
paps = https://<%= @pap_host %>:<%= @pap_port %>/pap/services/ProvisioningService
retentionInterval = <%= @pdp_retention_interval %>

[SECURITY]
serviceCertificate = <%= @certificate_path %>
servicePrivateKey = <%= @private_key_path %>
trustInfoDir = <%= @trust_info_dir %>
enableSSL = <%= @pdp_enable_ssl %>

